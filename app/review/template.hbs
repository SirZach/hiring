{{#if qualifiedApplications.length}}
  {{#paper-list}}
    {{#paper-subheader}}100% Correct Applicants{{/paper-subheader}}
    {{#each qualifiedApplications as |application|}}
      {{#paper-item as |controls|}}
        <p>{{application.name}}</p>
      {{/paper-item}}
      {{paper-divider}}
    {{/each}}
    {{#paper-item}}
      {{#paper-button onClick=(transition-to 'index')}}Go Home{{/paper-button}}
    {{/paper-item}}
  {{/paper-list}}
{{else}}
  {{#paper-card as |card|}}
    {{#card.title as |title|}}
      {{#title.text as |text|}}
        {{#text.headline}}No qualified applicants :({{/text.headline}}
        {{#text.subhead}}{{nonQualifiedApplicationsCount}} total applications{{/text.subhead}}
      {{/title.text}}
    {{/card.title}}
    {{#card.actions}}
      {{#paper-button primary=true onClick=(transition-to 'index')}}Go Home{{/paper-button}}
    {{/card.actions}}
  {{/paper-card}}
{{/if}}
